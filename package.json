{
  "name": "@sschepis/oboto",
  "version": "1.1.1",
  "type": "module",
  "description": "Oboto: AI-powered everything assistant",
  "author": "sschepis",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/sschepis/oboto.git"
  },
  "keywords": [
    "ai",
    "assistant",
    "agent",
    "automation",
    "llm",
    "dev-tool"
  ],
  "bin": {
    "oboto": "./ai.mjs"
  },
  "main": "src/lib/index.mjs",
  "module": "src/lib/index.mjs",
  "types": "src/lib/interfaces.d.ts",
  "sideEffects": false,
  "exports": {
    ".": {
      "types": "./src/lib/interfaces.d.ts",
      "import": "./src/lib/index.mjs",
      "default": "./src/lib/index.mjs"
    },
    "./cli": "./ai.mjs",
    "./lib": {
      "types": "./src/lib/interfaces.d.ts",
      "import": "./src/lib/index.mjs",
      "default": "./src/lib/index.mjs"
    },
    "./adapters": {
      "types": "./src/lib/interfaces.d.ts",
      "import": "./src/lib/adapters/index.mjs",
      "default": "./src/lib/adapters/index.mjs"
    },
    "./package.json": "./package.json"
  },
  "typesVersions": {
    "*": {
      ".": [
        "src/lib/interfaces.d.ts"
      ],
      "lib": [
        "src/lib/interfaces.d.ts"
      ],
      "adapters": [
        "src/lib/interfaces.d.ts"
      ]
    }
  },
  "files": [
    "ai.mjs",
    "src/**/*.mjs",
    "src/**/*.d.ts",
    "!src/**/__tests__/**",
    "!src/**/*.test.*",
    "!src/**/*.spec.*",
    "!src/client/**",
    "!src/gateway/**",
    "!src/service/**",
    "src/lib/README.md",
    "themes.json",
    "README.md",
    "LICENSE"
  ],
  "scripts": {
    "start": "node ai.mjs",
    "start:server": "node ai.mjs --server",
    "serve": "node src/server/server.mjs",
    "build:ui": "cd ui && pnpm install && pnpm run build",
    "dev:ui": "cd ui && pnpm run dev",
    "build:all": "pnpm install && pnpm run build:ui",
    "tray": "cd tray-app && npx electron .",
    "tray:dev": "cd tray-app && npx electron . --dev",
    "tray:install": "cd tray-app && npm install",
    "tray:build:mac": "cd tray-app && npx electron-builder --mac",
    "tray:build:win": "cd tray-app && npx electron-builder --win",
    "test": "NODE_OPTIONS='--experimental-vm-modules' jest",
    "check:deps": "node --input-type=module -e \"import{readFileSync}from'node:fs';const p=JSON.parse(readFileSync('./package.json','utf8'));if(Object.values(p.dependencies||{}).some(v=>v.includes('workspace:'))){console.error('ERROR: workspace: protocol deps found');process.exit(1)}else{console.log('Dependencies OK')}\"",
    "check:exports": "node --input-type=module -e \"import('./src/lib/index.mjs').then(m=>{const exports=['AiMan','Oboto','ConsoleStatusAdapter','NetworkLLMAdapter','MemoryAdapter','AiManEventBus','MiddlewareChain','CancellationError','DesignResult'];const missing=exports.filter(e=>!m[e]);if(missing.length){console.error('Missing exports:',missing);process.exit(1)}console.log('All',exports.length,'exports OK')}).catch(e=>{console.error('Library entry FAILED:',e.message);process.exit(1)})\"",
    "prepublishOnly": "npm run check:deps && npm run check:exports"
  },
  "dependencies": {
    "@aleph-ai/tinyaleph": "^1.7.3",
    "@anthropic-ai/vertex-sdk": "^0.14.3",
    "@google/genai": "^1.41.0",
    "@mlc-ai/web-llm": "^0.2.81",
    "@modelcontextprotocol/sdk": "^1.26.0",
    "@nut-tree-fork/nut-js": "^4.2.6",
    "@sschepis/resolang": "^0.5.0",
    "body-parser": "^2.2.2",
    "boxen": "^8.0.1",
    "chalk": "^5.5.0",
    "cheerio": "^1.2.0",
    "chokidar": "^5.0.0",
    "cli-spinners": "^3.2.0",
    "dotenv": "^17.3.1",
    "express": "^5.2.1",
    "figures": "^6.1.0",
    "gradient-string": "^3.0.0",
    "mathjs": "^14.6.0",
    "node-pty": "^1.1.0",
    "ora": "^8.2.0",
    "pdf-parse": "^1.1.1",
    "puppeteer": "^24.37.3",
    "react": "^19.2.4",
    "react-dom": "^19.2.4",
    "sharp": "^0.34.5",
    "vm2": "^3.10.4",
    "ws": "^8.19.0",
    "zod": "=3.25.76"
  },
  "optionalDependencies": {
    "@bubblelab/bubble-core": "*",
    "@bubblelab/bubble-runtime": "*",
    "@bubblelab/shared-schemas": "*",
    "@bubblelab/ts-scope-manager": "*"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "pnpm": {
    "onlyBuiltDependencies": [
      "canvas",
      "esbuild",
      "node-pty",
      "protobufjs",
      "puppeteer",
      "sharp",
      "unrs-resolver"
    ],
    "peerDependencyRules": {
      "ignoreMissing": [
        "@langchain/core"
      ],
      "allowAny": [
        "dotenv",
        "openai",
        "@mendable/firecrawl-js",
        "pdf-parse",
        "typescript"
      ]
    }
  },
  "devDependencies": {
    "@testing-library/jest-dom": "^6.9.1",
    "@testing-library/react": "^16.3.2",
    "@types/chokidar": "^1.7.5",
    "@types/jest": "^30.0.0",
    "@types/node": "^25.2.3",
    "@types/react": "^19.2.14",
    "@types/react-dom": "^19.2.3",
    "jest": "^30.2.0",
    "jest-environment-jsdom": "^30.2.0",
    "ts-jest": "^29.4.6"
  }
}
