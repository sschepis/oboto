{
  "name": "@sschepis/robodev",
  "version": "1.0.1",
  "type": "module",
  "description": "Robodev: AI-powered development assistant",
  "author": "sschepis",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/sschepis/robodev.git"
  },
  "keywords": [
    "ai",
    "assistant",
    "agent",
    "automation",
    "llm",
    "dev-tool"
  ],
  "bin": {
    "robodev": "./ai.mjs"
  },
  "main": "src/lib/index.mjs",
  "types": "src/lib/interfaces.d.ts",
  "exports": {
    ".": {
      "import": "./src/lib/index.mjs",
      "types": "./src/lib/interfaces.d.ts"
    },
    "./cli": "./ai.mjs",
    "./lib": {
      "import": "./src/lib/index.mjs",
      "types": "./src/lib/interfaces.d.ts"
    }
  },
  "files": [
    "ai.mjs",
    "src/**/*.mjs",
    "src/**/*.d.ts",
    "src/lib/README.md",
    "themes.json",
    "README.md",
    "LICENSE"
  ],
  "scripts": {
    "start": "node ai.mjs",
    "start:server": "node ai.mjs --server",
    "serve": "node src/server/server.mjs",
    "build:ui": "cd ui && pnpm install && pnpm run build",
    "dev:ui": "cd ui && pnpm run dev",
    "build:all": "pnpm install && pnpm run build:ui",
    "test": "jest",
    "prepublishOnly": "node -e \"const p=require('./package.json'); if(Object.values(p.dependencies||{}).some(v=>v.includes('workspace:'))){console.error('ERROR: workspace: protocol deps found');process.exit(1)}\" && node -e \"import('./src/lib/index.mjs').then(()=>console.log('Library entry OK')).catch(e=>{console.error('Library entry FAILED:',e.message);process.exit(1)})\""
  },
  "dependencies": {
    "@anthropic-ai/vertex-sdk": "^0.14.3",
    "@google/genai": "^1.41.0",
    "@modelcontextprotocol/sdk": "^1.26.0",
    "@nut-tree-fork/nut-js": "^4.2.6",
    "@sschepis/resolang": "^0.5.0",
    "body-parser": "^2.2.2",
    "boxen": "^8.0.1",
    "chalk": "^5.5.0",
    "chokidar": "^5.0.0",
    "cli-spinners": "^3.2.0",
    "dotenv": "^17.3.1",
    "express": "^5.2.1",
    "figures": "^6.1.0",
    "gradient-string": "^3.0.0",
    "mathjs": "^14.6.0",
    "node-pty": "^1.1.0",
    "ora": "^8.2.0",
    "puppeteer": "^24.37.3",
    "react": "^19.2.4",
    "react-dom": "^19.2.4",
    "vm2": "^3.10.4",
    "ws": "^8.19.0",
    "zod": "=3.25.76"
  },
  "optionalDependencies": {
    "@bubblelab/bubble-core": "*",
    "@bubblelab/bubble-runtime": "*",
    "@bubblelab/shared-schemas": "*",
    "@bubblelab/ts-scope-manager": "*"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "devDependencies": {
    "@testing-library/jest-dom": "^6.9.1",
    "@testing-library/react": "^16.3.2",
    "@types/chokidar": "^1.7.5",
    "@types/jest": "^30.0.0",
    "@types/node": "^25.2.3",
    "@types/react": "^19.2.14",
    "@types/react-dom": "^19.2.3",
    "jest": "^30.2.0",
    "jest-environment-jsdom": "^30.2.0",
    "ts-jest": "^29.4.6"
  }
}
